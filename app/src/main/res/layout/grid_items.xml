<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent" android:background="#000000">



    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000">

        <aloksharma.ufl.edu.stash.HoloCircularProgressBar
            android:id="@+id/holoCircularProgressBar"
            android:layout_gravity="center"
            android:layout_width="300dp"
            android:layout_height="300dp"/>



        <TextView
            android:layout_width="188dp"
            android:layout_height="158dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="30dp"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:text="Medium Text"
            android:id="@+id/stashName"
            android:layout_gravity="center_horizontal|bottom" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="100dp"
            android:text="25%"
            android:textColor="#FFFFFF"
            android:id="@+id/textView"
            android:layout_gravity="center" />
    </FrameLayout>
</LinearLayout>
